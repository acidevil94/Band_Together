<h1>Users</h1>
<br>
<div>
    <%= form_tag(users_index_path, :method => "get", id: "search-form") do %>
        <%= text_field_tag :search, params[:search], placeholder: "Search Users" %>
        <%= submit_tag "Search", :name => nil %>
    <% end %>
</div>
<br>
<div>
    <%= render partial: "users/search", locals: {users: @users} %>
</div>
